<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Admin.master.cs" Inherits="TEST_ASP_ALPHA_1.Admin" %>

<%@ Import Namespace="TEST_ASP_ALPHA_1.Common" %>

